<!doctype html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <!-- Bootstrap CSS -->
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.1/dist/css/bootstrap.min.css" rel="stylesheet"
        integrity="sha384-+0n0xVW2eSR5OomGNYDnhzAbDsOXxcvSN1TPprVMTNDbiYZCxYbOOl7+AMvyTG2x" crossorigin="anonymous">

    <!-- Google Fonts -->
    <link
        href="https://fonts.googleapis.com/css2?family=Montserrat:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&family=Ubuntu:ital,wght@0,300;0,400;0,500;0,700;1,300;1,400;1,500;1,700&display=swap"
        rel="stylesheet">

    <!-- Font Awesome -->
    <script src="https://kit.fontawesome.com/8a4b29bacf.js" crossorigin="anonymous"></script>
    <!-- jQuery CDN -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.5.1/jquery.min.js"></script>

    <link rel="stylesheet" href="/css/style.css">
    <title>The Assesor</title>
    <link rel="icon" href="/images/logo-white.png">
</head>
<body>
    <!-- Navbar -->
    <nav class="shadow-lg p-15 mb-1 rounded navbar navbar-expand-lg navbar-dark bg-success sticky-top">
        <div class="container-fluid">
            <a class="navbar-brand">
                <img src="<%= user.instituteLogo %> " alt="Logo" width="10%" height="10%"
                    class="d-inline-block align-text-top rounded-circle">
                <p class="d-inline-block align-text-top responsive-text cell" style="padding-left:2%;">
                    <%= user.instituteName %>
                </p>
            </a>
            <ul class="navbar-nav ml-* justify-content-end collapse navbar-collapse" id="navbarNavAltMarkup">
                <li>
                    <img src="<%= user.picture%>" alt="" width="60%" height="60%" loading=lazy
                        class="d-inline-block align-text-top rounded-circle d-none d-lg-block">
                </li>
            </ul>
        </div>
    </nav>